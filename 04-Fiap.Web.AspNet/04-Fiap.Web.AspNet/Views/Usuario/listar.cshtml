@model List<Fiap.Web.AspNet.Models.Usuario>
@{
    ViewData["Title"] = "listar";
}

<h1>Lista de Usuários</h1>

<table class="table">
    <tr>
        <th>Nome</th>
        <th>Data de Nascimento</th>
        <th>E-mail</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Nome</td>
            <td>@item.DataNascimento</td>
            <td>@item.Email</td>
        </tr>
    }
</table>

<a href="/Usuario/cadastrar"><input type="button" class="btn btn-primary" value="Voltar para tela de cadastro" /></a>

